{
    "name": "dmytrof/import-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle to import data",
    "keywords": ["Import"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Feshchenko",
            "email": "dmytro.feshchenko@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/http-client": "^4.3|^5.0",
        "symfony/serializer": "^4.3|^5.0",
        "dmytrof/models-management-bundle": "^1.0",
        "guzzlehttp/guzzle": "^6.4",
        "zendframework/zend-feed": "^2.12",
        "zendframework/zend-xml2json": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Dmytrof\\ImportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmytrof\\ImportBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
