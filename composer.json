{
    "name": "dmytrof/import-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle to import data",
    "keywords": ["Import"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Feshchenko",
            "email": "dmytro.feshchenko@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/http-client": "^4.3 || ^5.0",
        "symfony/lock": "^4.3 || ^5.0",
        "symfony/doctrine-bridge": "^4.3 || ^5.0",
        "dmytrof/models-management-bundle": "^1.7",
        "guzzlehttp/guzzle": "^6.4 || ^7.0",
        "laminas/laminas-feed": "^2.12",
        "laminas/laminas-xml2json": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3 || ^9.1"
    },
    "autoload": {
        "psr-4": {
            "Dmytrof\\ImportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dmytrof\\ImportBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
